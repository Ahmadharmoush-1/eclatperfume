            "content": "import { Mail, Phone, MapPin } from \"lucide-react\";\r\n\r\nconst Footer = () => {\r\n  const scrollToSection = (id: string) => {\r\n    const element = document.getElementById(id);\r\n    if (element) {\r\n      element.scrollIntoView({ behavior: \"smooth\" });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <footer className=\"bg-card border-t border-border\">\r\n      <div className=\"container mx-auto px-4 py-12\">\r\n        <div className=\"grid md:grid-cols-4 gap-8\">\r\n          {/* About */}\r\n          <div>\r\n            <h3 className=\"text-xl font-bold text-primary mb-4\">Dr. Smith Dental</h3>\r\n            <p className=\"text-muted-foreground\">\r\n              Transforming smiles with advanced cosmetic dentistry and compassionate care.\r\n            </p>\r\n          </div>\r\n\r\n          {/* Quick Links */}\r\n          <div>\r\n            <h4 className=\"text-lg font-semibold text-foreground mb-4\">Quick Links</h4>\r\n            <ul className=\"space-y-2\">\r\n              <li>\r\n                <button\r\n                  onClick={() => scrollToSection(\"home\")}\r\n                  className=\"text-muted-foreground hover:text-primary transition-colors\"\r\n                >\r\n                  Home\r\n                </button>\r\n              </li>\r\n              <li>\r\n                <button\r\n                  onClick={() => scrollToSection(\"about\")}\r\n                  className=\"text-muted-foreground hover:text-primary transition-colors\"\r\n                >\r\n                  About\r\n                </button>\r\n              </li>\r\n              <li>\r\n                <button\r\n                  onClick={() => scrollToSection(\"services\")}\r\n                  className=\"text-muted-foreground hover:text-primary transition-colors\"\r\n                >\r\n                  Services\r\n                </button>\r\n              </li>\r\n              <li>\r\n                <button\r\n                  onClick={() => scrollToSection(\"gallery\")}\r\n                  className=\"text-muted-foreground hover:text-primary transition-colors\"\r\n                >\r\n                  Gallery\r\n                </button>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n\r\n          {/* Services */}\r\n          <div>\r\n            <h4 className=\"text-lg font-semibold text-foreground mb-4\">Services</h4>\r\n            <ul className=\"space-y-2 text-muted-foreground\">\r\n              <li>Cosmetic Dentistry</li>\r\n              <li>Veneers</li>\r\n              <li>Teeth Whitening</li>\r\n              <li>Dental Implants</li>\r\n              <li>Smile Makeover</li>\r\n            </ul>\r\n          </div>\r\n\r\n          {/* Contact */}\r\n          <div>\r\n            <h4 className=\"text-lg font-semibold text-foreground mb-4\">Contact Info</h4>\r\n            <ul className=\"space-y-3\">\r\n              <li className=\"flex items-start gap-2 text-muted-foreground\">\r\n                <MapPin size={20} className=\"text-primary mt-1 flex-shrink-0\" />\r\n                <span>123 Smile Street, Suite 456<br />Dental City, DC 12345</span>\r\n              </li>\r\n              <li className=\"flex items-center gap-2 text-muted-foreground\">\r\n                <Phone size={20} className=\"text-primary flex-shrink-0\" />\r\n                <span>+1 (555) 123-4567</span>\r\n              </li>\r\n              <li className=\"flex items-center gap-2 text-muted-foreground\">\r\n                <Mail size={20} className=\"text-primary flex-shrink-0\" />\r\n                <span>info@drsmithdental.com</span>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"border-t border-border mt-8 pt-8 text-center text-muted-foreground\">\r\n          <p>&copy; {new Date().getFullYear()} Dr. Smith Dental. All rights reserved.</p>\r\n        </div>\r\n      </div>\r\n    </footer>\r\n  );\r\n};\r\n\r\nexport default Footer;\r\n"
